  "license": "MIT",
  "author": "wtetsu",
  "scripts": {
    "lint": "prettier --check src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-cov": "npm test -- --coverage",
