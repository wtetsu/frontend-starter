    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-select": "^3.1.0",
    "recharts": "^1.8.5",
    "sweetalert": "^2.1.2",
    "universal-router": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/react": "^16.9.41",
    "@types/react-datepicker": "^3.0.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^3.0.14",
    "@types/recharts": "^1.8.13",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
